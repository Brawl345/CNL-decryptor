{
  "name": "cnl-decryptor",
  "version": "2.0.0",
  "description": "Click'N'Load Decryptor",
  "author": "Brawl345",
  "license": "Unlicense",
  "repository": {
    "url": "https://github.com/Brawl345/CNL-decryptor/"
  },
  "type": "module",
  "scripts": {
    "dev": "node esbuild.js",
    "start:chrome": "web-ext run -t chromium",
    "build": "cross-env NODE_ENV=\"production\" node esbuild.js && web-ext build --overwrite-dest",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "web-ext-lint": "cross-env NODE_ENV=\"production\" node esbuild.js && web-ext lint",
    "global:check-dependencies": "npm-check --skip-unused"
  },
  "webExt": {
    "sourceDir": "./public/"
  },
  "devDependencies": {
    "chrome-types":"0.1.251",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-unicorn": "50.0.1",
    "esbuild": "^0.16.0",
    "prettier": "3.2.4",
    "web-ext": "7.10.0"
  },
  "dependencies": {
    "crypto-es": "2.1.0"
  }
}
